#include <iostream>
using namespace std;

int main(int argc, char *argv[]) {
	// definicion y asigancion de valores char.
	char letra;
	char vocal_may, vocal_min;
	letra='L';
	cout<<"imprimiendo la letra 'L':"<<letra<<endl;
	//letra="ll";//error por asignar mas de un caracter.
	//asignando valores numericos.
	letra=65; //ASCII  es letra A.
	cout<<"imprimiendo ASCII '65':"<<letra<<endl;
	// imprimir en codigo ASCII las vocales may y min con acentos.
	vocal_min=160;
	cout<<"imprimiendo ASCII '160':"<<vocal_min<<endl;
	vocal_min=130;
	cout<<"imprimiendo ASCII '130':"<<vocal_min<<endl;
	vocal_min=161;
	cout<<"imprimiendo ASCII '161':"<<vocal_min<<endl;
	vocal_min=162;
	cout<<"imprimiendo ASCII '162':"<<vocal_min<<endl;
	vocal_min=163;
	cout<<"imprimiendo ASCII '163':"<<vocal_min<<endl;
	vocal_may=181;
	cout<<"imprimiendo ASCII '181':"<<vocal_may<<endl;
	vocal_may=144;
	cout<<"imprimiendo ASCII '144':"<<vocal_may<<endl;
	vocal_may=214;
	cout<<"imprimiendo ASCII '214':"<<vocal_may<<endl;
	vocal_may=224;
	cout<<"imprimiendo ASCII '224':"<<vocal_may<<endl;
	vocal_may=233;
	cout<<"imprimiendo ASCII '233':"<<vocal_may<<endl;
	vocal_may=64;
	cout<<"imprimiendo ASCII '64':"<<vocal_may<<endl;
	vocal_may=92;
	cout<<"imprimiendo ASCII '92':"<<vocal_may<<endl;
	// declaracion de una cadena de caracteres.
	char nombre[]="SANDRO";
	
	cout<<"imprimiendo el nombre '/':"<<nombre<<endl;
	cout<<"imprimiendo el nombre[0] '/':"<<nombre[0]<<endl;
	cout<<"imprimiendo el nombre[1] '/':"<<nombre[1]<<endl;
	cout<<"imprimiendo el nombre[2] '/':"<<nombre[2]<<endl;
	cout<<"imprimiendo el nombre[3] '/':"<<nombre[3]<<endl;
	cout<<"imprimiendo el nombre[4] '/':"<<nombre[4]<<endl;
	cout<<"imprimiendo el nombre[5] '/':"<<nombre[5]<<endl;
	nombre[0]='s';
	nombre[1]='a';
	nombre[2]='n';
	nombre[3]='d';
	nombre[4]='r';
	nombre[5]='o';
	cout<<"imprimiendo el  nombre '/':"<<nombre<<endl;
	
	return 0;
}
